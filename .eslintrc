{
  "extends": "standard",
  "rules": {
    "array-bracket-spacing": "error",
    "no-lonely-if": "warn",
    "arrow-parens": "error",
    "no-var": "error",
    "prefer-const": "error",
    "max-len": [
      "warn",
      {
        "code": 155,
        "tabWidth": 2,
        "ignoreComments": true
      }
    ]
  }
}