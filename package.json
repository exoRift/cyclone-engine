{
  "name": "cyclone-engine",
  "version": "1.0.0",
  "description": "A Discord bot client manager/engine and command handler combo! Easy to use.",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter lcov ava --verbose **/*.test.js",
    "lint": "eslint ./",
    "build": "node build"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "packages/*/test/**",
      "test/**",
      "build/**",
      "test{,-*}.js",
      "**/*{.,-}test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/babel.config.js"
    ]
  },
  "files": [
    "lib/",
    "assets/",
    "!assets/src/",
    "!assets/icon.png",
    "!assets/splash.png"
  ],
  "author": "mets11rap",
  "license": "MIT",
  "dependencies": {
    "simple-knex": "git+https://github.com/alex-taxiera/simple-knex.git#development"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "dblapi.js": "^2.3.0",
    "dotenv": "^7.0.0",
    "eris": "^0.9.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "nyc": "^13.3.0",
    "pg": "^7.11.0",
    "sinon": "^7.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mets11rap/cyclone-engine.git"
  },
  "keywords": [
