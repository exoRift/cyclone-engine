{
  "name": "cyclone-engine",
  "version": "1.2.4",
  "description": "An easy to use Discord bot client manager/engine and event handler combo! Automate everything with a stable bot framework.",
  "main": "index.js",
  "scripts": {
    "test": "node ./ && nyc --reporter lcov ava --verbose **/*.test.js",
    "lint": "eslint ./",
    "build": "build.bat",
    "publishModule": "npm publish"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "packages/*/test/**",
      "test/**",
      "build/**",
      "test{,-*}.js",
      "**/*{.,-}test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/babel.config.js"
    ]
  },
  "files": [
    "lib/",
    "!**/*.test.js",
    "assets/",
    "!assets/src/",
    "!assets/icon.png",
    "!assets/splash.png"
  ],
  "author": "mets11rap",
  "license": "MIT",
  "dependencies": {
    "simple-knex": "git+https://github.com/alex-taxiera/simple-knex.git#development"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "dblapi.js": "^2.3.0",
    "dotenv": "^7.0.0",
    "eris": "^0.9.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc": "^3.6.3",
    "nyc": "^13.3.0",
    "pg": "^7.12.1",
    "readmetables-jsdoc-template": "^1.0.2",
    "sinon": "^7.4.1",
    "tui-jsdoc-template": "git+https://github.com/mets11rap/tui.jsdoc-template.git"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mets11rap/cyclone-engine.git"
  },
  "keywords": [
    "javascript",
    "discord",
    "bot",
    "discord-bot",
    "eris",
    "circle-ci",
    "knex",
    "simple-knex",
    "command-handler"
  ],
  "bugs": {
    "url": "https://github.com/mets11rap/cyclone-engine/issues"
  },
  "homepage": "https://mets11rap.github.io/cyclone-engine"
}
